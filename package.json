{
  "name": "SatVis",
  "version": "1.0.0",
  "description": "Bordeaux monitoring satellite orbit visualization with CesiumJS",
  "scripts": {
    "build": "npm run update-tle && webpack --config webpack/webpack.prod.js",
    "build:dev": "npm run update-tle && webpack --config webpack/webpack.config.js",
    "build:stats": "npm run update-tle && webpack --config webpack.stats.js",
    "clean": "mkdirp dist & rimraf dist/*",
    "lint": "eslint --ext js,vue src",
    "lint:fix": "eslint --ext js,vue --fix src",
    "serve": "npm run build && http-server -c-1 dist",
    "serve:dev": "npm run build:dev && http-server -c-1 dist",
    "serve:stats": "npm run build:stats && http-server -c-1 dist",
    "start": "npm run update-tle && webpack serve --progress --config webpack/webpack.config.js",
    "start:anyhost": "npm run update-tle && webpack serve --progress --config webpack/webpack.config.js --host 0.0.0.0",
    "update-tle": "node data/tle/update-tle.js",
    "update-custom-data": "npm run update-tle && bash data/custom/update-custom-data.sh"
  },
  "author": "Mike Rodden",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@primevue/themes": "^4.0.0",
    "@sentry/browser": "^8.15.0",
    "cesium": "^1.119.0",
    "cesium-sensor-volumes": "github:Flowm/cesium-sensor-volumes#1.102.0",
    "dayjs": "^1.11.11",
    "pinia": "^2.1.7",
    "primevue": "^4.0.0",
    "satellite.js": "^5.0.0",
    "vue": "^3.4.31",
    "vue-multiselect": "^3.0.0",
    "vue-router": "^4.4.0",
    "vue-toastification": "^2.0.0-rc.5",
    "workbox-window": "^7.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "cssnano": "^7.0.4",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-webpack": "^0.13.8",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.27.0",
    "html-webpack-plugin": "^5.6.0",
    "http-server": "^14.1.1",
    "mini-css-extract-plugin": "^2.9.0",
    "mkdirp": "^3.0.1",
    "postcss-loader": "^8.1.1",
    "rimraf": "^5.0.8",
    "sass": "^1.77.6",
    "sass-loader": "^14.2.1",
    "script-loader": "^0.7.2",
    "style-loader": "^4.0.0",
    "typescript": "^5.5.3",
    "url-loader": "^4.1.1",
    "vue-loader": "^17.4.2",
    "webpack": "^5.92.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^6.0.1",
    "webpack-strip-pragma-loader": "^1.0.1",
    "workbox-webpack-plugin": "^7.1.0"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all",
    "not UCAndroid >1"
  ]
}
